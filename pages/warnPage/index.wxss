/* pages/warnPage/index.wxss */
.wrapper{
  width:100%;
  background-color:#f2f2f2;
}
.wrapper .title{
  padding:25rpx 0 25rpx 25rpx;
  font-size:28rpx;
}
.wrapper .checkbox{
  background-color:white;
  display:flex;
  justify-content:space-around;
  flex-wrap:wrap;
  padding:20rpx 40rpx 40rpx 40rpx;
}
.checkbox .item{
  border:2rpx solid #FFE700;
  padding:15rpx 30rpx;
  border-radius:15rpx;
  margin-top:20rpx;
}
.wrapper .pic-content{
  background-color:white;
  font-size:20rpx;
  padding:20rpx;
  display: flex;
  flex-wrap: wrap;
}
.pic-content .item{
  border:2rpx solid black;
  border-radius:10rpx;
  text-align: center;
  width:120rpx;
  height: 120rpx;
  line-height: 120rpx;
  margin-left:20rpx;
}
.item:nth-of-type(1){
  margin-left:20rpx;
}
.item image{
  width:100%;
  height: 100%;
}
.wrapper .input-content{
  padding:30rpx;
  font-size:30rpx;
  background: white;
  margin-top:20rpx;
}
.input-content input{
  border-bottom:2rpx solid #f2f2f2;
  padding:20rpx 0;
}
.wrapper .btn-content{
  padding:20rpx;
}
